{
    "name": "hipster/user-discounts",
    "description": "Laravel package for user-level discounts with deterministic stacking",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hipster",
            "email": "dev@hipster.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/events": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Hipster\\UserDiscounts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hipster\\UserDiscounts\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hipster\\UserDiscounts\\UserDiscountsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

